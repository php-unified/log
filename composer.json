{
  "name": "php-unified/log",
  "description": "A PHP standard for logging.",
  "keywords": ["log"],
  "type": "library",
  "license": "MIT",
  "prefer-stable": true,
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "authors": [
    {
      "name": "PHP-SC",
      "role": "Developer"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "PhpUnified\\Log\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpUnified\\Log\\Tests\\": "tests/"
    }
  },
  "archive": {
    "exclude": [
      "/tests",
      "/.gitignore"
    ]
  }
}
